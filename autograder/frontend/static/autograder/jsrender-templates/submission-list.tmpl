<div>Group members: {{for data.attributes.members}}{{>}} {{/for}}</div>
<div class='panel-group' id='submission-list'>
    <div class='panel panel-default'>
    {{for included}}
        <div class='panel-heading'>
            <h4 class='panel-title'>
                <a data-toggle='collapse' class='collapsible-link' href="#submission-{{:~root.data.id}}-{{:id}}">
                    {{to_local_date:attributes.timestamp }}
                </a>
            </h4>
        </div>
        <div id='submission-{{:~root.data.id}}-{{:id}}' class='panel-collapse collapse submission-collapse'>
            <a href="{{:links.self}}" style='display:none'></a>
            <div class="panel-body"></div>
        </div>
    {{else}}
        You do not have any submissions yet
    {{/for}}
    </div>
</div>

